<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape>
      <stroke android:width="1dp" android:color="@color/colorDescription" />
      <corners android:radius="2dp" />
      <padding android:bottom="1dp" android:left="5dp" android:right="5dp" android:top="1dp" />
      <solid android:color="@android:color/transparent"/>
    </shape>
  </item>
  <item 
    android:top="1dp"
    android:bottom="1dp"
    android:right="1dp"
    android:left="1dp">
    <bitmap android:gravity="center_vertical|right" android:src="@drawable/flecha_s_vertical" />
  </item>
</layer-list>
